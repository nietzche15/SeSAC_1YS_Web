<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
    <title>Document</title>


    <script>
        function register(){
            var form = document.getElementById('form_register');
            var formData = new FormData(form); // 윗줄에서 지정한 form의 정보를 바탕으로 formtData 생성

            axios({
                method : 'post',
                url : '/dynamic',
                data : formData,
                headers : {
                    'Content-Type' : 'multipart/form-data'
                }
            }).then((a)=>{ console.log( 'a : ', a); return a.data; })
            .then((b)=>{
                console.log('b : ', b);
                document.querySelector('img').src= b.path;
            })
        };
    </script>
</head>
<body>
    <form id="form_register" enctype="multipart/form-data">
        <fieldset sttyle="width:300px;">
            <legend>Personal Info.</legend>
            ID : <input type="text" name="id"><br>
            PW : <input type="password" name="pw"><br>
            NAME : <input type="text" name="name"><br>
            AGE : <input type="text" name="age"><br>
        </fieldset>
        <input type="file" name="userfile">
        <button type="button" onclick="register();">Sign Up</button>
    </form><br>

    <img>

</body>
</html> 
